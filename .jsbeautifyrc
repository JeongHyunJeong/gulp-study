{
    "js": {
        "eol": "\r\n",
        "indent_size": 4,
        "indent_char": " ",
        "indent_level": 0,
        "indent_with_tabs": false,
        "wrap_line_length": 0,
        "braceStyle": "collapse",
        "end_with_newline": true
    },
    "html": {
        "eol": "\r\n",
        "brace_style": "collapse",
        "wrap_line_length": 0,
        "indent_size": 4,
        "indent_char": " ",
        "indent_level": 0,
        "end_with_newline": false,
        "content_unformatted": [ "pre" ],
        "unformatted": [ "hr", "pre", "br", "script" ]
    }
}
